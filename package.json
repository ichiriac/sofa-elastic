{
  "name": "sofa-elastic",
  "version": "0.0.1",
  "description": "Plugin based on Sofa-ODM for indexing and searching document with Couchbase and ElasticSearch",
  "main": "main.js",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha -t 10000",
    "coverage": "node ./node_modules/istanbul/lib/cli.js cover ./node_modules/sofa-odm/node_modules/mocha/bin/_mocha -- -R spec",
    "doc": "node ./node_modules/jsdox/bin/jsdox --output doc/api main.js src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ichiriac/sofa-elastic.git"
  },
  "keywords": [
    "elasticsearch",
    "couchbase",
    "sofa-odm",
    "fulltext"
  ],
  "author": "Ioan CHIRIAC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ichiriac/sofa-elastic/issues"
  },
  "homepage": "https://github.com/ichiriac/sofa-elastic",
  "dependencies": {
    "sofa-odm": "^0.1.3"
  },
  "devDependencies": {
    "mocha": "^1.21.4",
    "istanbul": "^0.3.0",
    "jsdox": "^0.4.2"
  }
}
